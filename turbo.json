{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist",
        ".wundergraph/generated/**"
      ],
      "env": [
        "WG_PUBLIC_NODE_URL",
        "ARBITRUM_SUBGRAPH_API_KEY",
        "UPSTASH_REDIS_URL",
        "UPSTASH_REDIS_TOKEN"
      ]
    },
    "lint": {},
    "test": {
      "dependsOn": [
        "^test"
      ],
      "env": [
        "ARBITRUM_SUBGRAPH_API_KEY",
        "UPSTASH_REDIS_URL",
        "UPSTASH_REDIS_TOKEN"
      ]
    },
    "build:release": {
      "dependsOn": [
        "^build:release"
      ],
      "cache": false
    },
    "publish-package": {
      "dependsOn": [
        "build:release"
      ],
      "env": [
        "ARBITRUM_SUBGRAPH_API_KEY",
        "UPSTASH_REDIS_URL",
        "UPSTASH_REDIS_TOKEN"
      ],
      "cache": false
    }
  }
}